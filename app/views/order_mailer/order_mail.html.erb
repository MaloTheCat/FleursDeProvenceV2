<h1><%= @order %></h1>
<hr>

<h3>Montant total =</h3>
<%= @order.amount_total/100 %>€<hr>

<h3>Order Customer ID =</h3>
<%= @order.customer.id %><hr>

<h3>nom customer =</h3>
<%= @order.customer.name %><hr>

<h3>Email customer =</h3>
<%= @order.customer.email %><hr>

<h3>phone customer =</h3>
<%= @order.customer.phone %><hr>

<h3>ID Order =</h3>
<%= @order.id %><hr>

<h3>ID du line_item : </h3>
<% @order.line_items.data.each do |line| %>
  <%= line %>
  <h3>NICKNAME =</h3>
  <%#= line.price.nickname %><hr>

  <h3>Nom de la composition :</h3>
  <%= line.description %><hr>

  <h3>Price ID de la composition :</h3>
  <%= line.price.id %><hr>

  <h3>Prix de la Composition :</h3>
  <%= line.price.unit_amount/100 %>€<hr>

<% end %>

